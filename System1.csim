create flux
	name StartFlux
	speed 23
	compounds H2O 70 NaCl 30

create flux
	name Flux1
	speed 13
	compounds H2O 70 NaCl 30

create flux
	name Flux2
	speed ?
	compounds H2O 70 NaCl 30

create machine
	name Machine1
	input StartFlux
	output Flux1 Flux2

create system
	name System1
	machines Machine1

run System1
save System1